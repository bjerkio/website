{
  "name": "website",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.9.2",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "postbuild": "npm run generate",
    "preview": "vite preview",
    "test": "vitest",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte --check . && eslint .",
    "format": "prettier --write --plugin prettier-plugin-svelte .",
    "generate": "tsx ./bin/generate-graph-images.ts && svelte-sitemap --domain https://bjerk.io"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.26.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^4.0.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vite-imagetools": "^5.0.4",
    "vitest": "^0.34.6",
    "zx": "^7.2.2"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/sora": "^5.0.1",
    "@portabletext/svelte": "^2.0.0",
    "@sanity/client": "^6.1.2",
    "@sanity/image-url": "^1.0.2",
    "groqd": "^0.15.9",
    "phosphor-svelte": "^1.3.0",
    "puppeteer-core": "^21.0.0",
    "sass": "^1.62.1",
    "sharp": "^0.32.5",
    "svelte-carbonbadge": "^0.0.1",
    "svelte-meta-tags": "^3.0.0",
    "svelte-preprocess": "^5.0.4",
    "svelte-sitemap": "^2.6.0",
    "ts-invariant": "^0.10.3"
  }
}
